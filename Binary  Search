def binary_search(arr, target):
    lb = 0
    ub = len(arr) - 1
    
    while lb <= ub:
        mid = (lb + ub) // 2   # integer division
        
        if arr[mid] == target:
            return mid   # return index if found
        elif arr[mid] < target:
            lb = mid + 1
        else:
            ub = mid - 1
    
    return -1  

arr = [1, 3, 5, 7, 9, 11]
target = 7
result = binary_search(arr, target)

if result != -1:
    print(f"Element found at index {result}")
else:
    print("Element not found")
